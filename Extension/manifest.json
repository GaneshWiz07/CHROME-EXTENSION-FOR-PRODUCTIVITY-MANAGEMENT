{
  "manifest_version": 3,
  "name": "FocusGuard",
  "version": "1.0.0",
  "description": "A productivity tracker that monitors website usage and helps maintain focus",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "alarms",
    "notifications"
  ],
  "icons": {
    "16": "productivity16.png",
    "48": "productivity48.png",
    "128": "productivity128.png"
  },
  "action": {
    "default_popup": "popup.html"
  

  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}